# GolfGod Mini - Supabase Database Configuration Template
# Copy this file to .env.local and fill in your Supabase credentials

# Supabase Connection (Required)
VITE_SUPABASE_URL="https://your-project-ref.supabase.co"
VITE_SUPABASE_ANON_KEY="your-anon-key"

# Direct Database Connection (Optional - for migrations/seeding)
VITE_DATABASE_URL="postgresql://postgres.your-project-ref:[password]@aws-region.pooler.supabase.com:5432/postgres"
VITE_DIRECT_URL="postgresql://postgres.your-project-ref:[password]@aws-region.pooler.supabase.com:5432/postgres"

# Example with actual project (DO NOT COMMIT WITH REAL VALUES):
# VITE_SUPABASE_URL="https://iqzmzyuqjcydqolkcqtr.supabase.co"
# VITE_SUPABASE_ANON_KEY="eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."

# Application Settings
NODE_ENV="development"
VITE_APP_NAME="GolfGod Mini"

# Optional: Enable/disable features
VITE_ENABLE_DEVTOOLS="true"
VITE_ENABLE_QUERY_DEVTOOLS="true"

# Optional: Logging level
LOG_LEVEL="info"